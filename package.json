{
  "name": "chrome-extension-boilerplate-react-vite-typescript-tailwind",
  "version": "0.0.1",
  "description": "chrome extension boilerplate",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/codegino/chrome-extension-react-vite-ts-tw.git"
  },
  "scripts": {
    "build": "tsc --noEmit && vite build",
